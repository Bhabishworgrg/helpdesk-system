<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="todo_app_todo_stage_view_list" model="ir.ui.view">
        <field name="name">todo_app.todo_stage.view.list</field>
        <field name="model">todo_app.todo_stage</field>
        <field name="arch" type="xml">
			<list editable="bottom">
				<field name="sequence" widget="handle"/>
				<field name="name"/>
			</list>
		</field>
	</record>
	
	<record id="action_todo_app_todo_stage" model="ir.actions.act_window">
		<field name="name">Todo Stage</field>
		<field name="res_model">todo_app.todo_stage</field>
		<field name="view_mode">list</field>
	</record>
</odoo>

