<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="helpdesk_app_helpdesk_category_view_list" model="ir.ui.view">
        <field name="name">helpdesk_app.helpdesk_category.view.list</field>
        <field name="model">helpdesk_app.helpdesk_category</field>
        <field name="arch" type="xml">
			<list editable="bottom">
				<field name="sequence" widget="handle"/>
				<field name="name"/>
				<field name="description"/>
			</list>
		</field>
	</record>
	
	<record id="action_helpdesk_app_helpdesk_category" model="ir.actions.act_window">
		<field name="name">Ticket Category</field>
		<field name="res_model">helpdesk_app.helpdesk_category</field>
		<field name="view_mode">list</field>
	</record>
</odoo>
